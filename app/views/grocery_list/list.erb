<p> Welcome, <%= current_user.username %>
  <h2> Here's your Grocery Lists </h2>

<% @lists.each do |list| %>
  <div>
    <h3><%= list.store_name %></h3>
    <p><%= list.item_list %>
    <a href="/grocery_list/<%=list.id%>/edit">Edit List </a>
    <form method="post" action="/grocery_list/<%=list.id%>/delete">
    <input id="hidden" type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete List">
  </form>
  </p>
  </div>
<% end %>
<a href="/grocery_list/new">Create a New List here</a>
<a href="/users/logout"> Logout </a>
